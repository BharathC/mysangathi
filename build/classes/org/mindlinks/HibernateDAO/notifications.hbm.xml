<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.mindlinks.HibernateDAO">
	<class name="org.mindlinks.Bean.Notifications" table="interestprofile">
		<id name="notificationid" type="int" column="notificationID">
			<generator class="increment"></generator>
		</id>

		<property name="matchingprofileid" type="int" column="matchingprofileID"></property>
		<property name="referenceid" type="int" column="referenceID"></property>
		<property name="name" type="string" column="name"></property>
		<property name="age" type="int" column="age"></property>
		<property name="education" type="string" column="education"></property>
		
		
		<property name="caste" type="String" column="caste"></property>
		<property name="city" type="String" column="city"></property>
		<property name="state" type="string" column="state"></property>
		<property name="contry" type="String" column="contry"></property>
		<property name="notificationdate"  column="notificationDate"></property>
		
		


		</class>
</hibernate-mapping>