<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.mindlinks.HibernateDAO">
	<class name="org.mindlinks.Bean.ProfileViews" table="profileviews">
		
		<id name="profileid" type="int" column="ProfileID">
			<generator class="increment"></generator>
		</id>
	<property name="fromprofileid" type="int" column="FromProfileid"></property>
		<property name="toprofileid" type="int" column="ToProfileid"></property>
		<property name="viewDate"  column="ViewDate"></property>

		</class>
</hibernate-mapping>