<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.mindlinks.HibernateDAO">
	<class name="org.mindlinks.Bean.PaymentRequestResponse" table="PaymentRequestResponse">
		<id name="reqResId" type="int" column="ReqResId">
			<generator class="increment"></generator>
		</id>

		<property name="profileId" type="Profile" column="ProfileID"></property>
		<property name="amount" type="double" column="Amount"></property>
		
		<property name="requestDate" type="Date" column="RequestDate"></property>
		<property name="requestData" type="String" column="RequestData"></property>
		<property name="responseData" type="String" column="ResponseData"></property>
		<property name="status" type="String" column="Status"></property>
		<property name="errorData" type="String" column="ErrorData"></property>
		<property name="vendor" type="String" column="Vendor"></property>
		<property name="sessionid" type="int" column="Sessionid"></property>
		<property name="transactionId" type="int" column="TransactionID"></property>
		<property name="createdDate" type="Date" column="CreatedDate"></property>
		<property name="lastModifiedDate" type="Date" column="LastModifiedDate"></property>


	</class>
</hibernate-mapping>