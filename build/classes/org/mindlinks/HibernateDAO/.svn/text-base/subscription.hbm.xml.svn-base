<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.mindlinks.HibernateDAO">
	<class name="org.mindlinks.Bean.Subscription" table="subscription">
		<id name="subscribedid" type="int" column="SubscribeID">
			<generator class="increment"></generator>
		</id>

		<property name="status" type="string" column="Status"></property>
		<property name="subcribeddate"  column="SubscribedDate"></property>
		<property name="features" type="string" column="Features"></property>
		<property name="username" type="string" column="UserName"></property>
		<property name="url" type="string" column="Url"></property>
		<property name="temppassword" type="string" column="TempPassword"></property>
		




	</class>
</hibernate-mapping>
